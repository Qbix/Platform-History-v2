<table class="Users_contacts_display">
{{#each contacts}}
	<tr>
		<td><i class='qp-communities-{{this.prefix}}'></i></td>
		<td><span>{{this.name}}</span></td>
		<td><i class='qp-communities-close' data-id="{{this.id}}"></i></td>
	</tr>
{{/each}}
</table>
<button class="Q_button Streams_invite_submit_contact">{{text.inviteSelectedContacts}}</button>