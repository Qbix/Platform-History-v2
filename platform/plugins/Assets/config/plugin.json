{
	"Q": {
		"pluginInfo": {
			"Assets": {
				"version": "1.0",
				"compatible": "0.7",
				"requires": {"Q": "0.9", "Streams": "0.9"},
				"connections": ["Assets"]
			}
		},
		"handlersBeforeEvent": {
			"Q/responseExtras": ["Assets/before/Q_responseExtras"]
		},
		"handlersAfterEvent": {
			"Users/insertUser": ["Assets/after/Users_insertUser"],
			"Assets/charge": ["Assets/after/Assets_charge"],
			"Assets/startSubscription": ["Assets/after/Assets_startSubscription"]
		},
		"routes": {
			"Q/plugins/Assets/:action": {"module": "Assets"},
			"Q/plugins/Assets/stripe/webhook": {"module": "Assets", "action": "stripeWebhook"}
		},
		"web": {
			"requireValidNonce": ["Assets"]
		},
		"bundle": {
			"exclude": {
				"Assets": ["img"]
			}
		},
		"text": {
			"Assets": {
				"*": ["Assets/content"]
			}
		},
		"views": {
			"Assets/charge.php": {"html": true},
			"Assets/charged.php": {"html": true},
			"Assets/startedSubscription.php": {"html": true},
			"Assets/startSubscription.php": {"html": true}
		}
	},
	"Streams": {
		"types": {
			"Assets/credits": {
				"description": "Keeps track of credits and payments",
				"defaults": {
					"title": "Subscription plan",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			},
			"Assets/plan": {
				"description": "Subscription plan",
				"create": true, 
				"edit": true,
				"defaults": {
					"title": "Subscription plan",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			},
			"Assets/subscription": {
				"description": "Subscription",
				"create": false, 
				"edit": false,
				"defaults": {
					"title": "Subscription",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			},
			"Assets/product": {
				"description": "Product",
				"create": false, 
				"edit": false,
				"defaults": {
					"title": "Product",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			},
			"Assets/service": {
				"description": "Service",
				"create": false, 
				"edit": false,
				"defaults": {
					"title": "Service",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			}
		}	
	},
	"Assets": {
		"credits": {
			"amounts": {
				"Users/insertUser": 20
			}
		},
		"currencies": {
			"ignoreCache": false,
			"dontCache": false,
			"duration": 3600
		},
		"reasons": {
			"Assets/purchased": {
				"by": "purchasing credits"
			},
			"Assets/received": {
				"by": "receiving credits"
			}
		},
		"transactional": {
			"charge": {
				"subject": "{{call 'publisher.displayName'}}: charge processed for {{call 'user.displayName'}}",
				"body": "Assets/email/charge.php",
				"sms": "Assets/sms/charge.php"
			},
			"charged": {
				"subject": "Thank you for your payment to {{call 'publisher.displayName'}}",
				"body": "Assets/email/charged.php",
				"sms": "Assets/sms/charged.php"
			},
			"startSubscription": {
				"subject": "{{call 'user.displayName'}} subscribed to {{plan.title}}",
				"body": "Assets/email/startSubscription.php",
				"sms": "Assets/sms/startSubscription.php"
			},
			"startedSubscription": {
				"subject": "You have been subscribed to {{plan.title}}",
				"body": "Assets/email/startedSubscription.php",
				"sms": "Assets/sms/startedSubscription.php"
			}
		},
		"browsertab": {
			"css": []
		}
	}
}
