{
	"Q": {
		"pluginInfo": {
			"Assets": {
				"version": "1.0.9",
				"compatible": "0.7",
				"requires": {"Q": "0.9", "Streams": "0.9"},
				"connections": ["Assets"]
			}
		},
		"handlersBeforeEvent": {
			"Q/responseExtras": ["Assets/before/Q_responseExtras"],
			"Streams/create/Assets/service": ["Assets/before/Streams_create_Assets_service"]
		},
		"handlersAfterEvent": {
			"Users/insertUser": ["Assets/after/Users_insertUser"],
			"Assets/charge": ["Assets/after/Assets_charge"],
			"Assets/startSubscription": ["Assets/after/Assets_startSubscription"],
			"Streams/invite/accept": ["Assets/after/Streams_inviteAccept"],
			"Db/Row/Streams_Stream/saveExecute": ["Assets/after/Streams_save"],
			"Streams/fetch/Assets/NFT/contract": ["Assets/after/Streams_fetch_Assets_NFT_contract"]
		},
		"routes@start": {
			"Assets/NFT/:publisherId/:streamId": {"module": "Assets", "action": "NFT"},
			"Assets/NFT/:tokenId": {"module": "Assets", "action": "NFT"},
			"Q/plugins/Assets/stripe/webhook": {"module": "Assets", "action": "stripeWebhook"},
			"Q/plugins/Assets/:action": {"module": "Assets"},
			"Assets/services": { "module": "Assets", "action": "services" },
			"Assets/NFTprofile/:userId": {"module": "Assets", "action": "NFTprofile"},
			"Assets/NFTprofile": {"module": "Assets", "action": "NFTprofile"},
			"Assets/NFTcontests": {"module": "Assets", "action": "NFTcontests"},
			"Assets/NFTowned": {"module": "Assets", "action": "NFTowned"},
			"Assets/NFTcontract": {"module": "Assets", "action": "NFTcontract"},
			"Q/plugins/Assets/webhooks/:serviceName": {"module": "Assets", "action": "blockchainWebhook"}
		},
		"web": {
			"requireValidNonce": ["Assets"]
		},
		"bundle": {
			"exclude": {
				"Assets": ["img"]
			}
		},
		"text": {
			"Assets": {
				"*": ["Assets/content"]
			}
		},
		"views": {
			"Assets/charge.php": {"html": true},
			"Assets/charged.php": {"html": true},
			"Assets/startedSubscription.php": {"html": true},
			"Assets/startSubscription.php": {"html": true}
		},
		"images": {
			"Assets/service": {
				"sizes": ["40", "50", "80", "200", "400", "400x", "x400", "x100", "500x"],
				"defaultSize": "400",
				"maxStretch": 2
			},
			"NFT/icon": {
				"sizes": ["300x","500x","700x","x"],
				"defaultSize": "300x",
				"defaultCacheBust": 1000,
				"maxStretch": 2
			},
			"NFT/series/icon": {
				"sizes": ["300x","500x","700x","x"],
				"defaultSize": "300x",
				"defaultCacheBust": 1000,
				"maxStretch": 2
			}
		}
	},
	"Streams": {
		"userStreams": {
			"Assets": "streams.json"
		},
		"types": {
			"Assets/credits": {
				"defaults": {
					"title": "Subscription plan",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			},
			"Assets/plan": {
				"create": true, 
				"edit": true,
				"defaults": {
					"title": "Subscription plan",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			},
			"Assets/subscription": {
				"create": false, 
				"edit": false,
				"defaults": {
					"title": "Subscription",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			},
			"Assets/product": {
				"create": false, 
				"edit": false,
				"defaults": {
					"title": "Product",
					"readLevel": 0,
					"writeLevel": 0,
					"adminLevel": 0
				}
			},
			"Assets/service": {
				"create": true,
				"edit": true,
				"defaults": {
					"title": "Assets Service",
					"readLevel": 40,
					"writeLevel": 23,
					"adminLevel": 0
				}
			},
			"Assets/NFT": {
				"create": true,
				"close": true,
				"edit": true,
				"defaults": {
					"title": "",
					"icon": "{{Assets}}/img/empty_white.png"
				}
			},
			"Assets/NFT/series": {
				"create": true,
				"close": true,
				"edit": true,
				"defaults": {
					"title": "",
					"icon": "{{Assets}}/img/empty_white.png"
				}
			},
			"Assets/NFT/contract": {
				"create": true,
				"close": true,
				"edit": true,
				"canCreate": ["Users/admins", "Users/owners"],
				"defaults": {
					"icon": "{{Assets}}/img/empty_white.png",
					"readLevel": 40,
					"writeLevel": 10,
					"adminLevel": 20
				}
			},
			"Assets/NFT/pointer": {
				"create": true,
				"close": true,
				"edit": true,
				"defaults": {
					"title": "NFT Pointer",
					"icon": "Streams/file"
				}
			},
			"Assets/contest": {
				"create": true,
				"close": true,
				"edit": true,
				"defaults": {
					"title": "",
					"icon": "Streams/category"
				}
			},
			"Assets/contest/round": {
				"create": true,
				"close": true,
				"edit": true,
				"defaults": {
					"title": "",
					"icon": "Streams/category"
				}
			}
		},
		"possibleUserStreams": {
			"Assets/user/NFTs": true,
			"Assets/NFT/contracts": true,
			"Assets/NFT/series": true,
			"Assets/contests": true
		},
		"onInsert": {
			"community": ["Assets/NFTs", "Assets/NFT/contracts", "Assets/contests"],
			"person": ["Assets/user/NFTs", "Assets/NFT/contracts", "Assets/NFT/series"]
		}
	},
	"Assets": {
		"payments": {
			"stripe": {
				"preloadAPI": true,
				"jsLibrary": "https://js.stripe.com/v2/"
			}
		},
		"credits": {
			"amounts": {
				"Users/insertUser": 20,
				"createCommunity": 20
			},
			"granted": {
				"acceptedInvite": 10,
				"invitedUserEntered": {
					"Streams/user/firstName": 1,
					"Streams/user/lastName": 1,
					"Streams/user/icon": 10,
					"Places/user/location": 10,
					"Streams/greeting/": 10
				}
			},
			"exchange": {
				"credits": 1,
				"USD": 10
			},
			"bonus": {
				"bought": {
					"500": 50, /* if user bought 500 credits at once, he will get 50 credits bonus */
					"1000": 150 /* if user bought 1000 credits at once, he will get 150 credits bonus */
				}
			}
		},
		"currencies": {
			"ignoreCache": false,
			"dontCache": false,
			"duration": 3600
		},
		"reasons": {
			"Assets/purchased": {
				"by": "purchasing credits"
			},
			"Assets/received": {
				"by": "receiving credits"
			}
		},
		"transactional": {
			"charge": {
				"subject": ["Assets/content", ["charge", "Subject"]],
				"body": "Assets/email/charge.php",
				"sms": "Assets/sms/charge.php"
			},
			"charged": {
				"subject": ["Assets/content", ["charged", "Subject"]],
				"body": "Assets/email/charged.php",
				"sms": "Assets/sms/charged.php"
			},
			"startSubscription": {
				"subject": ["Assets/content", ["notifications", "SubscribedTo"]],
				"body": "Assets/email/startSubscription.php",
				"sms": "Assets/sms/startSubscription.php"
			},
			"startedSubscription": {
				"subject": ["Assets/content", ["notifications", "YouSubscribedTo"]],
				"body": "Assets/email/startedSubscription.php",
				"sms": "Assets/sms/startedSubscription.php"
			}
		},
		"browsertab": {
			"css": []
		},
		"canCheckPaid": ["Users/owners", "Users/admins", "Assets/admins"],
		"NFT": {
			"contract": {
				"allow": {
					"author": true
				}
			},
			"attributes": {
				"display_type": [
					{
						"name": "Text",
						"value": "string"
					},
					{
						"name": "Date",
						"value": "date"
					},
					{
						"name": "Number",
						"value": "number"
					},
					{
						"name": "Relative Number",
						"value": "boost_number"
					},
					{
						"name": "Relative Percentage",
						"value": "boost_percentage"
					}
				]
			},
			"methods": {
				"tokensByOwner": {
					"limit": 100
				}
			},
			"URI": {
				"base": "{{baseUrl}}/Assets/NFT/{{contractId}}/{{tokenId}}",
				"suffix": ".json"
			},
			"currencies": [
				{
