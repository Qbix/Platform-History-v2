CREATE TABLE `{{prefix}}permalink` (
  `uri` varbinary(255) NOT NULL DEFAULT '',
  `url` varbinary(255) NOT NULL DEFAULT '',
  `insertedTime` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  `updatedTime` timestamp NULL DEFAULT NULL,
  PRIMARY KEY `URI` (`uri`),
  KEY `URL` (`url`)
) ENGINE=InnoDB COMMENT='For permalink lookup';