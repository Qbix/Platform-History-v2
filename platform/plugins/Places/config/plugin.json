{
	"Q": {
		"pluginInfo": {
			"Places": {
				"version": "0.9",
				"compatible": "0.4",
				"connections": ["Places"],
				"requires": {"Streams": "0.9"}
			}
		},
		"routes": {
			"plugins/Places/:action": {"module": "Places"}
		},
		"handlersBeforeEvent": {
			"Q/responseExtras": ["Places/before/Q_responseExtras"]
		},
		"handlersAfterEvent": {
			"Streams/interest/add": ["Places/after/Streams_interest_add"],
			"Streams/interest/remove": ["Places/after/Streams_interest_remove"]
		},
		"web": {
			"requireValidNonce": ["Places"]
		}
	},
	"Streams": {
		"types": {
			"Places/community": {
				"edit": true
			},
			"Places/location": {
				"create": true,
				"edit": true,
				"messages": {
					"Places/location/visit": { "post": true },
					"Places/location/stay": { "post": true }
				},
				"defaults": {
					"title": "Location"
				}
			},
			"Places/nearby": {},
			"Places/timeslot": {
				"defaults": {
					"title": "Nearby Timeslot"
				},
				"relatedTo": {
					"*": {
						"description": "New {{fromDisplayType}} scheduled in timeslot"
					}
				}
			},
			"Places/interest": {
				"defaults": {
					"title": "Nearby Interest"
				}
			},
			"Places/location": {
				"create": true,
				"edit": true,
				"messages": {
					"Places/location/visit": { "post": true },
					"Places/location/stay": { "post": true }
				},
				"defaults": {
					"title": "Location"
				},
				"relatedFrom": {
					"*": {
						"description": "Posted to an entire location"
					}
				}
			},
			"Places/area": {
				"edit": true,
				"messages": {
					"Places/location/visit": { "post": true },
					"Places/location/stay": { "post": true }
				},
				"defaults": {
					"title": "Area"
				},
				"relatedFrom": {
					"*": {
						"description": "Posted to a specific area"
					}
				}
			},
			"Places/floor": {
				"edit": true,
				"relatedFrom": {
					"*": {
						"description": "Posted to entire floor"
					}
				}
			},
			"Places/column": {
				"edit": true,
				"relatedFrom": {
					"*": {
						"description": "Posted to entire column"
					}
				}
			}
		}
	},
	"Places": {
		"nearby": {
			"miles": [1, 5, 10, 15, 20, 25, 50, 75, 100],
			"defaultMiles": 20,
			"minSubscribeMiles": 1,
			"maxSubscribeMiles": 15
		},
		"geolocation": {
			"requireLogin": false,
			"allowClientQueries": false,
			"timeLimit": 100000
		},
		"google": {
			"keys": {
				"server": null,
				"web": null
			}
		},
		"location": {
			"cache": {
				"duration": 2592000
			}
		}
	}
}